    "tsc-watch": "^4.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^17.0.1",
    "cross-env": "^7.0.3",
